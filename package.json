{
  "name": "vite-ssr-vue3",
  "version": "0.0.1",
  "description": "vite ssr plugin for vue3",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "vite-ssr": "./bin/vite-ssr.js"
  },
  "scripts": {
    "dev": "tsup --watch src",
    "build": "unbuild",
    "example:dev": "tsup && concurrently --kill-others -c 'cyan.bold,yellow.bold' -n 'SSR,Basic' \"pnpm dev\" \"pnpm -F basic dev\"",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --fix --ext .ts,.js .",
    "prepublishOnly": "npm run build",
    "release": "npx bumpp --push --tag --commit"
  },
  "keywords": [
    "vite",
    "ssr",
    "vue"
  ],
  "homepage": "https://github.com/aliuq/vite-ssr-vue3",
  "bugs": "https://github.com/aliuq/vite-ssr-vue3/issues",
  "author": "liuq",
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "*.d.ts"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.6",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/yargs": "^17.0.9",
    "bumpp": "^7.1.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "rollup": "^2.70.0",
    "tsup": "^5.12.0",
    "typescript": "^4.6.2",
    "unbuild": "0.6.9",
    "vite": "^2.8.6",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "fs-extra": "^10.0.1",
    "kolorist": "^1.5.1",
    "node-fetch": "^3.2.2",
    "serve-static": "^1.14.2",
    "yargs": "^17.3.1"
  },
  "peerDependencies": {
    "vite": "^2.8.6",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
